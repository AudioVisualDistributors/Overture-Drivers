          "default": "192.168.1.1"
        },
        "port": {
          "title": "Port:",
          "type": "integer",
          "default": 4999,
          "description": "Default: 4999"
        },
        "inputs": {
          "title": "Inputs:",
          "type": "integer",
          "default": 8,
          "enum": [1, 2, 3, 4, 5, 6, 7, 8]
        },
        "outputs": {
          "title": "Outputs:",
          "type": "integer",
          "default": 8,
          "enum": [1, 2, 3, 4, 5, 6, 7, 8]
        }
      },
      "required": [
        "host",
        "port",
        "inputs",
        "outputs"
      ]
    }
  }
}