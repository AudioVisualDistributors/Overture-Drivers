{
  "name": "toa_m9000m2",
  "version": "1.0.0-beta.6",
  "description": "Driver For Toa M-9000M2 mixing amplifier",
  "main": "index.js",
  "scripts": {
	  "start": "node index.js"
  },
  "author": "AudioVisualDistributors",
  "license": "MIT",
  "overture": {
    "brand": "TOA",
    "models": ["M-9000M2"],
    "type": "device",
    "subtype": "audiosystem",
    "variables": [
      {
        "name": "Status",
        "type": "enum",
        "enums": ["Disconnected", "Connected"]
      }
    ],

    "commands": [
    ],

    "pointSetupSchema": {
      "type": "object",
      "title": "Setup for TOA Mixer via GC RS232",
      "properties": {
        "host": {
